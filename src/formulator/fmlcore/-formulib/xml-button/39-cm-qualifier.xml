

<!--                                                                        -->
<!-- TOOLBAR: Content MathML: Qualifier Elements Templates                  -->
<!--                                                                        -->
<toolbar id="@t@CMQualifier@"
         group="Content"
         name="Qualifier elements"
         caption="Qualifier Elements" 
         image="images/buttons/cm_qualifier.bmp"
         image-title="images/title/t_cm_qualifier_n.bmp"
         image-title-hot="images/title/t_cm_qualifier_h.bmp"
         columns="4" >


  <!-- qualifier elements                                                   -->
  <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
  <button name="Qualifier elements: lowlimit, uplimit"
          id="@cm_qualifier_q1_ask" type="mathml"
          vertical-center-type="frame" vertical-center-value="1" >
    <math display='block'>
      <apply>
        <bvar __flag="0x10000000">
          <mrow __flag="0x20000000"></mrow>
        </bvar>
        <lowlimit><mrow __flag="0x20000000"></mrow></lowlimit>
        <uplimit><mrow __flag="0x20000000"></mrow></uplimit>
        <mrow __flag="0x20000000"></mrow>
      </apply>
    </math>
  </button>


  <!-- qualifier elements                                                   -->
  <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
  <button name="Qualifier elements: interval"
          id="@cm_qualifier_q2_ask" type="mathml"
          vertical-center-type="frame" vertical-center-value="1" >
    <math display='block'>
      <apply>
        <bvar __flag="0x10000000">
          <mrow __flag="0x20000000"></mrow>
        </bvar>
        <interval>
          <mrow __flag="0x20000000"></mrow>
          <mrow __flag="0x20000000"></mrow>
        </interval>
        <mrow __flag="0x20000000"></mrow>
      </apply>
    </math>
  </button>


  <!-- qualifier elements                                                   -->
  <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
  <button name="Qualifier elements: condition"
          id="@cm_qualifier_q3_ask" type="mathml"
          vertical-center-type="frame" vertical-center-value="1" >
    <math display='block'>
      <apply>
        <bvar __flag="0x10000000">
          <mrow __flag="0x20000000"></mrow>
        </bvar>
        <condition>
          <mrow __flag="0x20000000"></mrow>
        </condition>
        <mrow __flag="0x20000000"></mrow>
      </apply>
    </math>
  </button>


  <!-- qualifier elements                                                   -->
  <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
  <button name="Qualifier elements: domainofapplication"
          id="@cm_qualifier_q4_ask" type="mathml"
          vertical-center-type="frame" vertical-center-value="1" >
    <math display='block'>
      <apply>
        <bvar __flag="0x10000000">
          <mrow __flag="0x20000000"></mrow>
        </bvar>
        <domainofapplication>
          <mrow __flag="0x20000000"></mrow>
        </domainofapplication>
        <mrow __flag="0x20000000"></mrow>
      </apply>
    </math>
  </button>


</toolbar>
