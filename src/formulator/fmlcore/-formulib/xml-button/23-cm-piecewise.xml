

<!--                                                                        -->
<!-- TOOLBAR: Content MathML: Piecewise Declaration Templates               -->
<!--                                                                        -->
<toolbar id="@t@Piecewise@"
         group="Content"
         name="Piecewise declaration" 
         caption="Piecewise" 
         image="images/buttons/cm_piecewise.bmp"
         image-title="images/title/t_cm_piecewise_n.bmp"
         image-title-hot="images/title/t_cm_piecewise_h.bmp"
         columns="4" >


  <!-- piecewise                                                            -->
  <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
  <button name="The piecewise element constructs 'piecewise' declarations of functions"
          stretchy="yes" 
          id="@piecewise1_" type="mathml"
          vertical-center-type="frame" vertical-center-value="1" >
    <math display='block'>
      <piecewise>
        <piece>
          <mrow __flag="0x20000000"></mrow>
          <mrow __flag="0x20000000"></mrow>
        </piece>
      </piecewise>
    </math>
  </button>


