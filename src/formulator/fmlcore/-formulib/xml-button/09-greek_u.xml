

<!--                                                                        -->
<!-- TOOLBAR: Greek characters (uppercase)                                  -->
<!--                                                                        -->
<toolbar id="@t@GreekUp@"
         group="Presentation"
         name="Greek characters (uppercase)"
         caption="Greek Up" 
         image="images/buttons/greek_u.bmp"
         image-title="images/title/t_greek_u_n.bmp" 
         image-title-hot="images/title/t_greek_u_h.bmp" >

  <!-- Greek characters (uppercase)                                         -->
  <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
  <button name="Greek capital letter alpha (Ctrl+G,Shift+A)"
          type="text" value="&amp;#x0391;" style="mathextra" id="Agr" >
    <hot-keys>
      <hot-key>
        <sequence ctrl ="yes" key="G" />
        <sequence shift="yes" key="A" />
      </hot-key>
    </hot-keys>
    <mathml value="&Agr;" type="mi" />
    <dsr head="" tail="$g " separator="" param="$child_list$" bracket="off" />
  </button>

