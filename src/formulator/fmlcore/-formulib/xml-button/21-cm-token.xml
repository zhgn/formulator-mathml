

<!--                                                                        -->
<!-- TOOLBAR: Content MathML: token elements templates                      -->
<!--                                                                        -->
<toolbar id="@t@Token@"
         group="Content"
         name="Token elements" 
         caption="Token" 
         image="images/buttons/cm_token.bmp"
         image-title="images/title/t_cm_token_n.bmp" 
         image-title-hot="images/title/t_cm_token_h.bmp" 
		 columns="2" >

  <!-- cn                                                                   -->
  <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
  <button name="The cn element is used to specify actual numerical constants" id="@cn" type="mathml" 
          vertical-center-type="frame" vertical-center-value="1" >
    <math display='block'>
        <cn></cn>
    </math>
  </button>

  <!-- cn                                                                   -->
  <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
  <button name="The cn element is used to specify actual numerical constants" id="@cn_input" type="mathml" 
          vertical-center-type="frame" vertical-center-value="1"
		  encoding="MathML-Content">
    <math display='block'>
        <cn></cn>
    </math>
  </button>

  <!-- cn                                                                   -->
  <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
  <button name="The cn element for a rational constant" id="@cn_rational" type="mathml" 
          vertical-center-type="object" vertical-center-value="1" 
		  encoding="MathML-Content">
    <math display='block'>
        <cn type="rational"><sep/></cn>
    </math>
  </button>

  <!-- cn                                                                   -->
  <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
  <button name="The cn element for a complex-cartesian constant" id="@cn_complex_cartesian" type="mathml" 
          vertical-center-type="frame" vertical-center-value="1"
		  encoding="MathML-Content">
    <math display='block'>
        <cn type="complex-cartesian"><sep/></cn>
    </math>
  </button>

  <!-- cn                                                                   -->
  <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
  <button name="The cn element for a complex-polar constant" id="@cn_complex_polar" type="mathml" 
          vertical-center-type="frame" vertical-center-value="1"
		  encoding="MathML-Content">
    <math display='block'>
        <cn type="complex-polar"><sep/></cn>
    </math>
  </button>

  <!-- ci                                                                   -->
  <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
  <button name="The ci element is used to name an identifier in an expression" id="@ci" type="mathml" 
          vertical-center-type="frame" vertical-center-value="1" >
    <math display='block'>
        <ci></ci>
    </math>
  </button>

  <!-- ci                                                                   -->
  <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
  <button name="The ci element is used to name an identifier in an expression" id="@ci_input" type="mathml" 
          vertical-center-type="frame" vertical-center-value="1"
		  encoding="MathML-Content">
    <math display='block'>
        <ci></ci>
    </math>
  </button>

  <!-- csymbol                                                              -->
  <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
  <button name="The csymbol element is used to create an element whose semantics are externally defined " id="@csymbol" type="mathml" 
          vertical-center-type="frame" vertical-center-value="1" >
    <math display='block'>
        <csymbol></csymbol>
    </math>
  </button>

</toolbar>
