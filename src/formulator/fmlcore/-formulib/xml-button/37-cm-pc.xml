

<!--                                                                        -->
<!-- TOOLBAR: Content MathML: Semantics Templates                           -->
<!--                                                                        -->
<toolbar id="@t@CMSemantics@"
         group="Content"
         name="Semantics"
         caption="Semantics" 
         image="images/buttons/cm_pc.bmp"
         image-title="images/title/t_cm_pc_n.bmp"
         image-title-hot="images/title/t_cm_pc_h.bmp" >

  <!-- semantics                                                            -->
  <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
  <button name="The semantics element associates additional representations with a given MathML construct"
          id="@cm_semantics" type="mathml"
          vertical-center-type="frame" vertical-center-value="1" >
    <math display='block'>
      <semantics __flag="0x10000000">
        <mrow __flag="0x20000000"></mrow>
        <annotation-xml encoding="MathML-Presentation">
          <mrow></mrow>
        </annotation-xml>
        <annotation-xml encoding="MathML-Content">
          <mrow __flag="0x20000000"></mrow>
        </annotation-xml>
      </semantics>
    </math>
  </button>


</toolbar>
